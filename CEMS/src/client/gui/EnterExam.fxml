<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane prefHeight="768.0" prefWidth="754.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.gui.StudentEnterExamController">
   <children>
      <JFXTextField fx:id="tfCode" layoutX="300.0" layoutY="303.0" prefHeight="31.0" prefWidth="129.0" promptText="Code" />
      <JFXButton fx:id="btComputerizedExam" layoutX="239.0" layoutY="353.0" onAction="#OnClickEnterComputerizedExam" style="-fx-background-color: #48a832; -fx-background-radius: 10;" text="Computerized exam" textFill="WHITE" />
      <JFXButton fx:id="btManualExam" layoutX="407.0" layoutY="353.0" onAction="#OnClickEnterManualExam" style="-fx-background-color: #48a832; -fx-background-radius: 10;" text="Manual exam" textFill="WHITE" />
      <Label fx:id="lbl_Status" layoutX="260.0" layoutY="458.0" textFill="RED" />
      <Label fx:id="lbTitle" layoutX="266.0" layoutY="49.0" text="Enter exam">
         <font>
            <Font name="System Bold Italic" size="36.0" />
         </font>
      </Label>
      <TextArea fx:id="taInstructions" layoutX="90.0" layoutY="133.0" prefHeight="72.0" prefWidth="501.0" text="Please enter the exam code you have given by your teacher&#10;and then click on the desired exam type.">
         <font>
            <Font size="18.0" />
         </font>
      </TextArea>
   </children>
</Pane>
