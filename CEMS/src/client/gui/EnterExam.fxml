<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane prefHeight="768.0" prefWidth="754.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.gui.StudentEnterExamController">
   <children>
      <JFXTextField fx:id="tfCode" layoutX="391.0" layoutY="328.0" promptText="Code" />
      <JFXButton fx:id="btComputerizedExam" layoutX="205.0" layoutY="446.0" onAction="#OnClickEnterComputerizedExam" style="-fx-background-color: #48a832; -fx-background-radius: 10;" text="Computerized exam" textFill="WHITE" />
      <JFXButton fx:id="btManualExam" layoutX="411.0" layoutY="446.0" onAction="#OnClickEnterManualExam" style="-fx-background-color: #48a832; -fx-background-radius: 10;" text="Manual exam" textFill="WHITE" />
      <Label fx:id="lbl_Status" layoutX="260.0" layoutY="539.0" textFill="RED" />
      <Label fx:id="lbTitle" layoutX="266.0" layoutY="49.0" text="Enter exam">
         <font>
            <Font name="System Bold Italic" size="36.0" />
         </font>
      </Label>
      <TextArea fx:id="taInstructions" editable="false" layoutX="90.0" layoutY="133.0" prefHeight="87.0" prefWidth="547.0" text="Please enter your ID  and the exam code you have given by your &#10;teacher and then click on the desired exam type.">
         <font>
            <Font size="18.0" />
         </font>
      </TextArea>
      <JFXTextField fx:id="tfUserID" layoutX="193.0" layoutY="328.0" promptText="ID" />
   </children>
</Pane>
