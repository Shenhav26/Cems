      <Label layoutX="253.0" layoutY="32.0" text="Create Exam" textFill="#3d9a26">
         <font>
            <Font size="39.0" />
         </font>
      </Label>
      <TableView fx:id="tvQuestions" layoutX="27.0" layoutY="126.0" prefHeight="558.0" prefWidth="700.0">
         <columns>
            <TableColumn fx:id="tcID" prefWidth="77.0" style="-fx-alignment: center;" text="ID" />
            <TableColumn fx:id="tcSubject" prefWidth="141.0" style="-fx-alignment: center;" text="Subject" />
            <TableColumn fx:id="tcTeacher" prefWidth="138.0" style="-fx-alignment: center;" text="Teacher" />
            <TableColumn fx:id="tcPoints" prefWidth="95.0" style="-fx-alignment: center;" text="Points" />
            <TableColumn fx:id="tcDetails" prefWidth="128.0" style="-fx-alignment: center;" text="Details" />
            <TableColumn fx:id="tcNote" prefWidth="116.0" style="-fx-alignment: center;" text="Note" />
         </columns>
      </TableView>
      <JFXButton fx:id="btnBack" layoutX="39.0" layoutY="704.0" onAction="#onClickBack" prefHeight="31.0" prefWidth="120.0" style="-fx-background-color: #48a832; -fx-background-radius: 10;" text="Back to edit" textFill="WHITE" />
      <Label layoutX="30.0" layoutY="100.0" text="Exam questions:" textFill="#333333">
         <font>
            <Font name="Calibri" size="18.0" />
         </font>
      </Label>
      <JFXButton fx:id="btnCreate" layoutX="595.0" layoutY="704.0" onAction="#onClickCreate" prefHeight="31.0" prefWidth="120.0" style="-fx-background-color: #48a832; -fx-background-radius: 10;" text="Create Exam" textFill="WHITE" />
   </children>
</Pane>
