<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>


<Pane prefHeight="600.0" prefWidth="230.0"
	style="-fx-background-color: #333333;"
	xmlns:fx="http://javafx.com/fxml/1"
	xmlns="http://javafx.com/javafx/8.0.171">
	<children>
		<ImageView fitHeight="85.0" fitWidth="200.0" layoutX="84.0"
			layoutY="56.0" pickOnBounds="true" preserveRatio="true">
			<image>
				<Image url="@../../../GuiPrototype/logo3.jpeg" />
			</image>
		</ImageView>
		<VBox layoutX="2.0" layoutY="215.0" prefHeight="247.0"
			prefWidth="227.0" style="-fx-background-color: #333333;">
			<children>
				<JFXButton fx:id="btnCreateTest" prefHeight="300.0"
					prefWidth="500.0" ripplerFill="#48a832"
					style="-fx-background-color: #333333;" text="Create Exam"
					textFill="WHITE">
					<graphic>
						<ImageView fitHeight="16.0" fitWidth="16.0"
							pickOnBounds="true" preserveRatio="true">
							<image>
								<Image url="@../../../GuiPrototype/test.png" />
							</image>
						</ImageView>
					</graphic>
				</JFXButton>
				<JFXButton fx:id="btnCreateQuestion" prefHeight="300.0"
					prefWidth="500.0" ripplerFill="#48a832"
					style="-fx-background-color: #333333;" text="Create Question"
					textFill="WHITE">
					<graphic>
						<ImageView fitHeight="16.0" fitWidth="13.0"
							pickOnBounds="true" preserveRatio="true">
							<image>
								<Image url="@../../../GuiPrototype/question.png" />
							</image>
						</ImageView>
					</graphic>
				</JFXButton>
				<JFXButton fx:id="btnStartTest" prefHeight="300.0"
					prefWidth="500.0" ripplerFill="#48a832"
					style="-fx-background-color: #333333;" text="Start Exam"
					textFill="WHITE">
					<graphic>
						<ImageView fitHeight="16.0" fitWidth="16.0"
							pickOnBounds="true" preserveRatio="true">
							<image>
								<Image url="@../../../GuiPrototype/start.png" />
							</image>
						</ImageView>
					</graphic>
				</JFXButton>
				<JFXButton fx:id="btnStatistic" prefHeight="300.0"
					prefWidth="500.0" ripplerFill="#48a832"
					style="-fx-background-color: #333333;" text="Test Statistic"
					textFill="WHITE">
					<graphic>
						<ImageView fitHeight="16.0" fitWidth="16.0"
							pickOnBounds="true" preserveRatio="true">
							<image>
								<Image url="@../../../GuiPrototype/statistic.png" />
							</image>
						</ImageView>
					</graphic>
				</JFXButton>
			</children>
		</VBox>
	</children>
</Pane>
