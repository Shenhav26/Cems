<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXComboBox?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.Pane?>

<Pane prefHeight="768.0" prefWidth="754.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.gui.CreateExamController">
	<children>
		<TableView fx:id="tvQuestionPull" layoutX="27.0" layoutY="74.0" prefHeight="300.0" prefWidth="700.0">
			<columns>
				<TableColumn fx:id="tcIdPull" prefWidth="137.0" text="ID" />
				<TableColumn fx:id="tcSubjectPull" prefWidth="170.0" text="Subject" />
				<TableColumn fx:id="tcTeacherPull" prefWidth="175.0" text="Teacher" />
				<TableColumn fx:id="tcAddPull" prefWidth="112.0" text="Add" />
				<TableColumn fx:id="tcDetailsPull" prefWidth="103.0" text="Details" />
			</columns>
		</TableView>
		<TableView fx:id="tvSelectedQuestion" layoutX="27.0" layoutY="411.0" prefHeight="300.0" prefWidth="700.0">
			<columns>
				<TableColumn fx:id="tcIdSelected" prefWidth="147.0" text="ID" />
				<TableColumn fx:id="tcSubjectSelected" prefWidth="162.0" text="Subject" />
				<TableColumn fx:id="tcTeacherSelected" prefWidth="186.0" text="Teacher" />
				<TableColumn fx:id="tcDetailsSelected" prefWidth="204.0" text="Details" />
			</columns>
		</TableView>
		<Label layoutX="37.0" layoutY="45.0" text="Question pull:" />
		<Label layoutX="37.0" layoutY="384.0" text="Selected question:" />
		<JFXTextField fx:id="tfDuration" focusColor="#48a832" layoutX="400.0" layoutY="723.0" promptText="Exam Duration" />
		<JFXButton fx:id="btnContinue" layoutX="595.0" layoutY="723.0" onAction="#onClickContinue" style="-fx-background-color: #48a832; -fx-background-radius: 10;" text="Continute" textFill="WHITE" />
		<JFXComboBox fx:id="cbQuestionSubject" focusColor="#48a832" layoutX="305.0" layoutY="14.0" onAction="#onSubjectSelected" prefHeight="28.0" prefWidth="145.0" promptText="Question Subject" />
		<JFXComboBox fx:id="cbExamCourse" focusColor="#48a832" layoutX="221.0" layoutY="723.0" prefHeight="28.0" prefWidth="145.0" promptText="Exam Course" />
		<JFXComboBox fx:id="cbExamSubject" focusColor="#48a832" layoutX="37.0" layoutY="723.0" onAction="#onClickExamSubject" prefHeight="28.0" prefWidth="145.0" promptText="Exam Subject" />
	</children>
</Pane>
